<div class="main-sponsors cont">
  <% @footer_sponsors.each do |sponsor|  %>
    <% unless sponsor.image.blank? %>
        <div class="main-sponsors-single">
            <span class="main-sponsors-centrer">
              <%= link_to image_tag(sponsor.image.url, alt: "Sea Grunt" ), sponsor.link %>
            </span>
        </div>
      <% end %>
  <% end %>
</div>

<footer class="main-footer cont">
  <div class="main-footer-top-header big-cont">
  </div>
  <div class="main-footer-content">
    <div class="main-footer-content-copyright">
      <a href="#" class="main-footer-content-copyright-place">North Carolina Catch</a>
      <p class="main-footer-content-copyright-text">Copyright Â© 2016 | Privacy Policy</p>
    </div>
      <% @footer_links.each_slice(6) do |group| %>
        <div class="main-footer-list">
          <% group.each do |link| %>
            <%= link_to link.title, refinery.page_path(link), class: "main-footer-list-element" %>
          <% end %>
        </div>
      <% end %>
      <div class="main-footer-menu-socials">
        <%= render partial: "refinery/social" %>
      </div>
  </div>
</footer>

<script>
  $('div').find('.main-footer-list').last().append($('.main-footer-menu-socials'))
</script>