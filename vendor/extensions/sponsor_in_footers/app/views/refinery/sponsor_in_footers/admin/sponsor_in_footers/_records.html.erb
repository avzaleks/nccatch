<%= render 'refinery/admin/search_header', :url => refinery.sponsor_in_footers_admin_sponsor_in_footers_path %>
<div class='pagination_container'>
  <% if @sponsor_in_footers.any? %>
    <%= render 'sponsor_in_footers' %>
  <% else %>
    <p>
      <% if searching? %>
        <%= t('no_results', :scope => 'refinery.admin.search') %>
      <% else %>
        <strong>
          <%= t('.no_items_yet') %>
        </strong>
      <% end %>
    </p>
  <% end %>
</div>
