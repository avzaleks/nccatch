<%= render 'refinery/admin/search_header', :url => refinery.explore_regional_nc_fishings_admin_explore_regional_nc_fishings_path %>
<div class='pagination_container'>
  <% if @explore_regional_nc_fishings.any? %>
    <%= render 'explore_regional_nc_fishings' %>
  <% else %>
    <p>
      <% if searching? %>
        <%= t('no_results', :scope => 'refinery.admin.search') %>
      <% else %>
        <strong>
          <%= t('.no_items_yet') %>
        </strong>
      <% end %>
    </p>
  <% end %>
</div>
