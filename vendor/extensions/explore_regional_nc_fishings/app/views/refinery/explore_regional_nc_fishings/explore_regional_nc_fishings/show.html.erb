<% content_for :body_content_title do %>
  <%= @explore_regional_nc_fishing.link %>
<% end %>

<% content_for :body do %>
  <section>
    <h1>Link</h1>
    <p>
      <%=raw @explore_regional_nc_fishing.link %>
    </p>
  </section>
  <section>
    <h1>Add To Home Page</h1>
    <p>
      <%=raw @explore_regional_nc_fishing.add_to_home_page %>
    </p>
  </section>
  <section>
    <h1>Image</h1>
    <p>
      <%= image_fu @explore_regional_nc_fishing.image, nil %>
    </p>
  </section>
<% end %>

<% content_for :side_body do %>
  <aside>
    <h2><%= t('.other') %></h2>
    <ul id="explore_regional_nc_fishings">
      <% @explore_regional_nc_fishings.each do |explore_regional_nc_fishing| %>
        <li>
          <%= link_to explore_regional_nc_fishing.link, refinery.explore_regional_nc_fishings_explore_regional_nc_fishing_path(explore_regional_nc_fishing) %>
        </li>
      <% end %>
    </ul>
  </aside>
<% end %>

<%= render '/refinery/content_page' %>
