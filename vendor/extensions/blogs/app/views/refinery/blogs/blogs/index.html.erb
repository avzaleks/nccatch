<main class="main-content cont" data-style="display: flex;">
  <section class="whowe">
    <h1>Other NC Catch`s posts</h1>
	  <ul id="blogs">
	    <% @blogs.each do |msg| %>
	      <li>
	        <%= link_to truncate( msg.title, length: 100, escape: false), refinery.blogs_blog_path(msg), class: "recipes-figure-link" %>
	      </li>
	    <% end %>
	  </ul>

	</section>
	<%= render partial: "refinery/pages/events" %>
</main>
